<launch>
	<node name="state" pkg="update" type="state.py"/>
	
	<node name="map_visualizer" pkg="map_server" type="map_visualizer.py" />
	
	<node name="parking_car" pkg="obstacles" type="parking_car.py" />
	<node name="collision_check" pkg="obstacles" type="collision_check_with_visualizing_car.py" />

	<node name="path" pkg="cv_agents" type="path_global.py" output="screen"/>
	<!-- <node name="path_select" pkg="cv_agents" type="path_mode_select_qualifier_kcity_parking.py" /> -->
	<node name="path_select" pkg="cv_agents" type="path_mode_select_final_kcity.py" />
	<node name="control" pkg="cv_agents" type="control.py" output="screen"/>
	<!-- <node name='core_controller' pkg='core' type='core_qualifier_kcity.py'/> -->
	<node name='core_controller' pkg='core' type='core_final_kcity.py'/>
	<node type="rviz" name="rviz" pkg="rviz" args="-d $(find cv_agents)/rviz/kcity.rviz"/>
	
</launch>